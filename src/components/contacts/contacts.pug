block contacts
  .contacts
    .contacts__header
      +block__header('./src/assets/img/contacts-icon.jpg', 'Contact')
    .contacts__main
      .contacts__title_wrap
        p(class="title-wrap") <b>ADDRESS</b>
        p(class="title-wrap") <b>E-MAIL</b>
        p(class="title-wrap") <b>PHONE</b>
        p(class="title-wrap") <b>WEBSITE</b>
      .contacts__text_wrap
        p.contacts__text Main Street, City.
        p.contacts__text contact@domain.com
        p.contacts__text 555-555-555
        p.contacts__text www.yourweb.com